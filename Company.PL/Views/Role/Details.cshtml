@model RoleViewModel
@{
    ViewData["Title"] = "Details";
}


<h1 class="my-5 w-75">Role @Model.RoleName Details</h1>


<!-- General Tab Content -->
<div>

    <div class="row">
        <div class="form-group col-12">
            <label class="col-form-label" asp-for="Id"></label>
            <input asp-for="Id" class="form-control" readonly />
        </div>
        <div class="form-group col-12">
            <label class="col-form-label" asp-for="RoleName"></label>
            <input asp-for="RoleName" class="form-control" readonly />
        </div>
    </div>
</div>


<div class="mt-4">
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-success">Edit</a>
    <a asp-action="Index" class="btn btn-outline-dark">Back To list</a>
</div>



